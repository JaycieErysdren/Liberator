{
  "name": "liberator",
  "version": "0.1.1",
  "description": "Obscure file format viewer.",
  "main": "dist_server/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaycieErysdren/Liberator.git"
  },
  "author": "Jaycie Erysdren",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JaycieErysdren/Liberator/issues"
  },
  "homepage": "https://github.com/JaycieErysdren/Liberator#readme",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "electron": "^20.0.3"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "jimp": "^0.16.1",
    "kaitai-struct": "^0.10.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "ignore": ["local/*", "meta/*"]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Liberator"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "maintainer": "Jaycie Erysdren",
              "homepage": "https://erysdren.me/"
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
